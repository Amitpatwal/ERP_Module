@page
@model POModel
@{

    ViewData["Title"] = "Home page";

}

<body>
    <div>
        <br />
        <div class="container-fluid">
            <div class="row">
                @*---------------Sale Order Reports-------------*@
                <div class="col-12 col-sm-6 col-md-2 ">
                    <div class="info-box mb-3 special" onclick="SaleOrderDatatable()">
                        <span class="info-box-icon bg-info elevation-1" style="width:60px"><i class="fa fa-file-text-o"></i></span>
                        <div class="info-box-content">
                            <span class="info-box-text">Sale Order List (<b id="saleorderlabel">0</b>) </span>
                            @* <span class="info-box-number" id="saleorderlabel">0</span>*@
                            <a class="small-box-footer">More <i class="fas fa-arrow-circle-right"></i></a>
                        </div>
                    </div>
                </div>

                @*---------------Daily Planning Report-------------*@
                <div class="col-12 col-sm-6 col-md-2 ">
                    <div class="info-box mb-3 special" onclick="DailyPlanningReport()">
                        <span class="info-box-icon success elevation-1" style="width:60px;background-color:magenta"><i style="color:white" class="fa fa-bar-chart-o"></i></span>
                        <div class="info-box-content">
                            <span class="info-box-text">Daily Report (<b id="dailyplanningnumber">0 </b>)</span>
                            @*<span class="info-box-number" id="dailyplanningnumber">0</span>*@
                            <a class="small-box-footer">More <i class="fas fa-arrow-circle-right"></i></a>
                        </div>
                    </div>
                </div>

                @*---------------Waiting For Despatch-------------*@
                <div class="col-12 col-sm-6 col-md-2">
                    <div class="info-box mb-3 special" onclick="WaitingDespatchReport()">
                        <span class="info-box-icon success elevation-1" style="width:60px ; background-color:brown"><i style="color:white" class="fas fa-shopping-cart"></i></span>
                        <div class="info-box-content">
                            <span class="info-box-text">Waiting  Despatch  (<b id="waitinglabel">0 </b>)</span>
                            @*<span class="info-box-number" id="waitinglabel">0</span> &nbsp;&nbsp;&nbsp;*@
                            <a class="small-box-footer">More  <i class="fas fa-arrow-circle-right"></i></a>
                        </div>
                    </div>
                </div>

                @*---------------Pending Material-------------*@
                <div class="col-12 col-sm-6 col-md-2">
                    <div class="info-box mb-3 special" onclick="PendingMaterialOrder()">
                        <span class="info-box-icon bg-success elevation-1" style="width:60px"><i class="fas fa-stopwatch"></i></span>
                        <div class="info-box-content">
                            <span class="info-box-text">Pending Material  (<b id="pendinglabel">0 </b>)</span>

                            @* <span class="info-box-number" id="pendinglabel">0</span> *@
                            <a class="small-box-footer">More  <i class="fas fa-arrow-circle-right"></i></a>
                        </div>
                    </div>
                </div>

                @*---------------Cancelled Order-------------*@
                <div class="col-12 col-sm-6 col-md-2">
                    <div class="info-box mb-3 special" onclick="cancelOrder()">
                        <span class="info-box-icon bg-danger elevation-1" style="width:60px"><i class="fa fa-close"></i></span>
                        <div class="info-box-content">
                            <span class="info-box-text">Cancelled (<b id="cancelLabel">0 </b>) </span>
                            @*  <span class="info-box-number" id="">0</span>*@
                            <a class="small-box-footer">More  <i class="fas fa-arrow-circle-right"></i></a>
                        </div>
                    </div>
                </div>

                @*---------------Hold Order-------------*@
                <div class="col-12 col-sm-6 col-md-2">
                    <div class="info-box mb-3 special" onclick="holdorder()">
                        <span class="info-box-icon bg-warning elevation-1" style="width:60px"><i class="fa fa-pause"></i></span>
                        <div class="info-box-content">
                            <span class="info-box-text">Hold Order (<b id="holdlabel">0 </b>)</span>
                            @*  <span class="info-box-number" id="holdlabel"></span>*@
                            <a class="small-box-footer">More<i class="fas fa-arrow-circle-right"></i></a>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        @*---------------Title-------------*@
        <nav class="navbar navbar-expand-sm  navbar-dark" id="TitleColor" style="background-color: #17a2b8; ">
            <ul class="navbar-nav">
                <li class="nav-item active">
                    <a class="nav-link" style="color:white " id="TitleID">Sales Order Reports</a>
                </li>
