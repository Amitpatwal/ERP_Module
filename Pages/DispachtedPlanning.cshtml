        @*-------------------POP UP  Planning Create/Update--------------------*@
        <div class="modal fade" id="exampleModal2" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
            <div class="modal-dialog modal-lg" role="document">
                <div class="modal-content">
                    <div class="modal-header" style="background-color:darkslategrey">
                        <span style="color:white">Order Planning</span>
                        <button style="color:white" type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                    <div class="modal-body">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <span>Order Planning No:</span>
                                            <input id="dpnumber" type="text" disabled class="form-control">
                                            <input id="fromm" type="text" hidden class="form-control">
                                            <input id="DONO" type="text" hidden class="form-control">
                                        </div>
                                    </div>

                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <span> Date</span>
                                            <input type="datetime-local" id="dpdate" style="width:100%" />
                                        </div>
                                    </div>

                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <span>P.O. Number</span>
                                            <input type="text" class="form-control" disabled id="ponumber" />
                                        </div>
                                    </div>

                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <span>S.O. Number</span>
                                            <input type="text" disabled class="form-control" id="sonumber" />
                                        </div>
                                    </div>

                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <span>Customer Name</span>
                                            <select class="form-control" id="customername" disabled style="width:100%;">
                                            </select>
                                        </div>
                                    </div>

                                    <div class="col-md-6">
                                        <div class="input-group">
                                            <span>Supplier Name</span>
                                            <select class="form-control" id="suppliername" style="width:100%;">
                                            </select>
                                        </div>
                                    </div>

                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <span>Dispachted Location</span>
                                            <input type="text" class="form-control" id="dispacthedlocation" />
                                        </div>
                                    </div>

                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <span>Dispatch Organized By</span>
                                            <input type="text" class="form-control" id="dispacthedincharge" />
                                        </div>
                                    </div>

                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <span>Status</span>
                                            <select class="form-control" id="status" style="width:100%;" onchange="holdreason()">
                                                <option>--Select Status--</option>
                                                <option value="dispatch">Proceed to Despatch</option>
                                                <option value="hold">Order Hold</option>
                                                <option value="cancel">Order Cancelled</option>
                                            </select>
                                        </div>
                                    </div>

                                    <div class="col-md-6" id="holdd" style="display:none;">
                                        <div class="form-group">
                                            <span>Hold Reason</span>
                                            <select class="form-control" id="holdreason" style="width:100%;">
                                                <option>--Select Hold Reason--</option>
                                                <option> Material Awaited</option>
                                                <option>Further Order Awaited</option>
                                                <option>Payment Awaited</option>
                                                <option>Inspection Awaited</option>
                                            </select>
                                        </div>
                                    </div>


                                    <div class="col-md-12">
                                        <div class="form-group">
                                            <span>Remarks</span>
                                            <input type="text" class="form-control" id="remarks" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>

                    <div class="modal-footer">
                        <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#exampleModal16" onclick="attachments()"> <i class="fa fa-paperclip" style="font-size:20px"></i> View Attachments</button>
                        <button type="button" class="btn btn-primary" onclick="deleteplanning()" id="deletePlanning" style="display:none">Delete</button>
                        <button type="button" class="btn btn-primary" data-toggle="modal" style="display:none" data-target="#exampleModal15" onclick="materialList()" id="MaterialListt">Material List</button>
                        <button type="button" class="btn btn-primary" onclick="saveplanning()" id="savePlanning">Save</button>

                    </div>
                </div>
            </div>
        </div>


