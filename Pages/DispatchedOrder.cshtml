                                                <div class="col-md-12">
                                                    <div class="row">
                                                        <input type="number" id="altquantity" class="form-control" placeholder="0.00" onkeyup="negativestock()" style="width:40%" />
                                                        <input type="text" id="altquantityunit" disabled class="form-control" placeholder="mtr" style="width:25%" />
                                                        <input type="text" id="Altqtystock" disabled class="form-control" placeholder="1540 mtr" style="width:35%" />
                                                        <input type="text" id="altvalue" hidden />
