                                <div class="col-md-12" style="background-color:black ;width:250px;height:2px"></div>
                                <div class="col-md-12" style="background-color:white ;width:250px;height:20px"></div>
                                <div class="col-md-12"><center><span><u> Received Material</u></span></center></div>

                                <div class="col-md-12" style="background-color:white ;width:250px;height:20px"></div>

                                <div class="col-md-3">
                                    <div class="form-group">
                                        <span>Quantity</span>
                                        <div class="row">
                                            <input type="number" id="quantity" class="form-control" placeholder="0.00" onkeyup="calcy('Purchase')" onkeypress="nextfocus(event,'altquantity')" style="width:70%" />
                                            <input type="text" id="quantityunit" disabled class="form-control" placeholder="mtr" style="width:30%" />
                                        </div>
                                    </div>
                                </div>

                                <div class="col-md-3" id="altqtyy" style="display:none;">
                                    <div class="form-group">
                                        <span>Alt Quantity</span>
                                        <div class="row">
                                            <input type="number" id="altquantity" class="form-control" placeholder="0.00" onkeyup="OnvalueChange()" onkeypress="nextfocus(event,'weight')" style="width:70%" />
                                            <input type="text" id="altquantityunit" disabled class="form-control" placeholder="nos" style="width:30%" />
                                            <input type="text" id="altvalue" hidden />
                                        </div>
                                    </div>
                                </div>

                                <div class="col-md-3" id="itemweight" style="display:none">
                                    <div class="form-group">
                                        <span>Item Weight</span>
                                        <div class="row">
                                            <input type="number" id="weight" class="form-control" placeholder="0.00" onkeyup="OnvalueChange()" onkeypress="nextfocus(event,'unloadedby')" style="width:70%" />
                                            <input type="number" id="weightt" hidden class="form-control" placeholder="0.00" onkeypress="nextfocus(event,'unloadedby')" style="width:70%" />
                                            <input type="text" id="weightunit" disabled class="form-control" value="KG" style="width:30%" />
                                        </div>
                                    </div>
                                </div>

                                <div class="col-md-3">
                                    <div class="input-group">
                                        <span>Unloaded By</span>
                                        <select class="form-control" id="unloadedby" onkeypress="nextfocus(event,'saveebutton1')" style="width: 100%;">
                                            <option>None</option>
                                            <option>Crane</option>
                                            <option>Manual</option>
                                            <option>Crane & Manual</option>
                                        </select>
                                    </div>
                                </div>

                                <div class="col-md-3">
                                    <div class="form-group">
                                        <span>Heat No /Batch No</span>
                                        <input id="heatno" type="text" onkeyup="this.value = this.value.toUpperCase();" class="form-control" />
                                    </div>
                                </div>

                                <div class="col-md-3">
                                    <div class="form-group">
                                        <span>Price (Primary.)</span>
                                        <div class="row">
                                            <input id="pricePRIN" type="text" disabled class="form-control" style="width:60%" />
                                            <input id="pricePR" type="text" disabled class="form-control" style="width:30%" />
                                        </div>
                                    </div>
                                </div>

                                <div class="col-md-3" style="display:none" id="scPriceDiv">
                                    <div class="form-group">
                                        <span>Price (Secondary)</span>
                                        <div class="row">
                                            <input id="priceSecIN" type="text" disabled class="form-control" style="width:60%" />
                                            <input id="priceSec" type="text" disabled class="form-control" style="width:30%" />
                                        </div>
                                    </div>
                                </div>

                                <div class="col-md-3" style="display:none" id="weightPriceDIv">
                                    <div class="form-group">
                                        <span> Price (Weight)</span>
                                        <div class="row">
                                            <input id="priceThIN" type="text" disabled class="form-control" style="width:60%" />
                                            <input id="priceTh" type="text" disabled class="form-control" style="width:30%" />
                                        </div>
                                    </div>
                                </div>


                                <div class="col-md-3">
                                    <div class="form-group">
                                        <span>Total Amount</span>
                                        <div class="row">
                                            <span type="text" style="width:20%" class="form-control">₹</span>
                                            <input id="totalAmount" type="number" class="form-control" onkeyup="OnvalueChange()" style="width:80%" />

                                        </div>
                                    </div>
                                </div>

                            </div>
                        </div>

                        <div class="modal-footer" id="modelfotter">
                            <div style="position: absolute; left: 0px;">
                                <span>Current Balance :</span>
                                <span id="current"></span>
                            </div>
                            <div>
                                <button type="button" class="btn btn-primary" onclick="addprdetails()" id="saveebutton1">Save</button>
                                <button id="resetitemm" type="button" class="btn btn-danger" onclick="dataitem(0)">Reset</button>
                            </div>
                           
                        </div>
                    </div>
                </form>
                    </div>
           
                </div>

